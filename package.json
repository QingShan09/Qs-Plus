{
  "name": "qs-plus",
  "version": "1.0.0",
  "type": "module",
  "description": "A Vue 3 UI Component Library",
  "main": "dist/qs-plus.umd.js",
  "module": "dist/qs-plus.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:lib": "vite build --config vite.config.lib.ts",
    "type-check": "vue-tsc --noEmit",
    "build-only": "vite build",
    "prepublishOnly": "npm run build:lib"
  },
  "keywords": [
    "vue3",
    "component",
    "ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@iconify/vue": "^4.2.0",
    "@vueuse/core": "^12.2.0",
    "highlight.js": "^11.11.1",
    "scss": "^0.2.4",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/tsconfig": "^0.7.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.69.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vue-tsc": "^2.2.0"
  }
}
